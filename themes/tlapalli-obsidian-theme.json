  "tokenColors": [
    {
      "scope": "variable.parameter.function",
      "settings": { "foreground": "#cd0808ee" }
    },
    {
      "scope": "comment, punctuation.definition.comment",
      "settings": { "foreground": "#5a5a5aad" }
    },
    {
      "scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.parameters, punctuation.definition.array",
      "settings": { "foreground": "#e0e0e098" }
    },
    { "scope": "none", "settings": { "foreground": "#a5a29e" } },
    { "scope": "keyword.operator", "settings": { "foreground": "#fafafa" } },
    {
      "scope": "keyword",
      "settings": { "foreground": "#8a8a8aee", "fontStyle": "italic" }
    },
    { "scope": "variable", "settings": { "foreground": "#d9d9d9ee" } },
    {
      "scope": "entity.name.function, meta.require, support.function.any-method",
      "settings": { "foreground": "#f7f7f7", "fontStyle": "italic" }
    },
    {
      "scope": "support.class, entity.name.class, entity.name.type.class",
      "settings": { "foreground": "#8f8c89" }
    },
    { "scope": "meta.class", "settings": { "foreground": "#ddd9d4" } },
    {
      "scope": "keyword.other.special-method",
      "settings": { "foreground": "#5e5c5a" }
    },
    { "scope": "storage", "settings": { "foreground": "#8a8a8aee" } },
    { "scope": "support.function", "settings": { "foreground": "#797774" } },
    {
      "scope": "string, constant.other.symbol, entity.other.inherited-class",
      "settings": { "foreground": "#e3e2e2b6" }
    },
    { "scope": "constant.numeric", "settings": { "foreground": "#d4d0cb" } },
    { "scope": "none", "settings": { "foreground": "#888582" } },
    { "scope": "none", "settings": { "foreground": "#888582" } },
    { "scope": "constant", "settings": { "foreground": "#c6c2bd" } },
    { "scope": "entity.name.tag", "settings": { "foreground": "#5d5d5d" } },
    {
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#888582" }
    },
    {
      "scope": "entity.other.attribute-name.id, punctuation.definition.entity",
      "settings": { "foreground": "#5e5c5a" }
    },
    { "scope": "meta.selector", "settings": { "foreground": "#8a8a8aee" } },
    { "scope": "none", "settings": { "foreground": "#888582" } },
    {
      "scope": "markup.heading punctuation.definition.heading, entity.name.section",
      "settings": { "foreground": "#5e5c5a", "fontStyle": "" }
    },
    { "scope": "keyword.other.unit", "settings": { "foreground": "#888582" } },
    {
      "scope": "markup.bold, punctuation.definition.bold",
      "settings": { "foreground": "#8f8c89", "fontStyle": "bold" }
    },
    {
      "scope": "markup.italic, punctuation.definition.italic",
      "settings": { "foreground": "#8a8a8aee", "fontStyle": "italic" }
    },
    { "scope": "markup.raw.inline", "settings": { "foreground": "#e0e0e0bf" } },
    {
      "scope": "string.other.link, punctuation.definition.string.end.markdown",
      "settings": { "foreground": "#6c6a67" }
    },
    { "scope": "meta.link", "settings": { "foreground": "#888582" } },
    { "scope": "markup.list", "settings": { "foreground": "#6c6a67" } },
    { "scope": "markup.quote", "settings": { "foreground": "#888582" } },
    { "scope": "meta.separator", "settings": { "foreground": "#a5a29e" } },
    { "scope": "markup.inserted", "settings": { "foreground": "#e0e0e0bf" } },
    { "scope": "markup.deleted", "settings": { "foreground": "#6c6a67" } },
    { "scope": "markup.changed", "settings": { "foreground": "#8a8a8aee" } },
    {
      "scope": "constant.other.color",
      "settings": { "foreground": "#797774" }
    },
    { "scope": "string.regexp", "settings": { "foreground": "#797774" } },
    {
      "scope": "constant.character.escape",
      "settings": { "foreground": "#797774" }
    },
    {
      "scope": "punctuation.section.embedded, variable.interpolation",
      "settings": { "foreground": "#8a8a8aee" }
    },
    { "scope": "invalid.illegal", "settings": { "foreground": "#ddd9d4" } },
    { "scope": "invalid.broken", "settings": { "foreground": "#020202" } },
    { "scope": "invalid.deprecated", "settings": { "foreground": "#ddd9d4" } },
    {
      "scope": "invalid.unimplemented",
      "settings": { "foreground": "#ddd9d4" }
    }
  ]
}
