	"tokenColors": [
		{
			"scope": "variable.parameter.function",
			"settings": { "foreground": "#ff7a85cc" }
		},
		{
			"scope": "comment, punctuation.definition.comment",
			"settings": { "foreground": "#6d4a4aaa" }
		},
		{
			"scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.parameters, punctuation.definition.array",
			"settings": { "foreground": "#ffb3b898" }
		},
		{ "scope": "none", "settings": { "foreground": "#e85d7a" } },
		{ "scope": "keyword.operator", "settings": { "foreground": "#ff8599" } },
		{ "scope": "keyword", "settings": { "foreground": "#c94d5aee", "fontStyle": "italic" } },
		{ "scope": "variable", "settings": { "foreground": "#ff7a85cc" } },
		{
			"scope": "entity.name.function, meta.require, support.function.any-method",
			"settings": { "foreground": "#df4c4c", "fontStyle": "italic" }
		},
		{
			"scope": "support.class, entity.name.class, entity.name.type.class",
			"settings": { "foreground": "#ff7070", "fontStyle": "italic" }
		},
		{ "scope": "meta.class", "settings": { "foreground": "#ffc4c9" } },
		{
			"scope": "keyword.other.special-method",
			"settings": { "foreground": "#7f3d3d" }
		},
		{ "scope": "storage", "settings": { "foreground": "#c94d5aee" } },
		{ "scope": "support.function", "settings": { "foreground": "#c94d5a" } },
		{
			"scope": "string, constant.other.symbol, entity.other.inherited-class",
			"settings": { "foreground": "#ffb3b8ee" }
		},
		{ "scope": "constant.numeric", "settings": { "foreground": "#ff9199" } },
		{ "scope": "none", "settings": { "foreground": "#df4c4c" } },
		{ "scope": "none", "settings": { "foreground": "#df4c4c" } },
		{ "scope": "constant", "settings": { "foreground": "#ff9199" } },
		{ "scope": "entity.name.tag", "settings": { "foreground": "#804a4a" } },
		{
			"scope": "entity.other.attribute-name",
			"settings": { "foreground": "#df4c4c" }
		},
		{
			"scope": "entity.other.attribute-name.id, punctuation.definition.entity",
			"settings": { "foreground": "#7f3d3d" }
		},
		{ "scope": "meta.selector", "settings": { "foreground": "#c94d5aee" } },
		{ "scope": "none", "settings": { "foreground": "#df4c4c" } },
		{
			"scope": "markup.heading punctuation.definition.heading, entity.name.section",
			"settings": { "foreground": "#7f3d3d", "fontStyle": "" }
		},
		{ "scope": "keyword.other.unit", "settings": { "foreground": "#df4c4c" } },
		{
			"scope": "markup.bold, punctuation.definition.bold",
			"settings": { "foreground": "#ff7070", "fontStyle": "bold" }
		},
		{
			"scope": "markup.italic, punctuation.definition.italic",
			"settings": { "foreground": "#c94d5aee", "fontStyle": "italic" }
		},
		{ "scope": "markup.raw.inline", "settings": { "foreground": "#ffb3b8ee" } },
		{
			"scope": "string.other.link, punctuation.definition.string.end.markdown",
			"settings": { "foreground": "#c94d5a" }
		},
		{ "scope": "meta.link", "settings": { "foreground": "#df4c4c" } },
		{ "scope": "markup.list", "settings": { "foreground": "#c94d5a" } },
		{ "scope": "markup.quote", "settings": { "foreground": "#df4c4c" } },
		{ "scope": "meta.separator", "settings": { "foreground": "#e85d7a" } },
		{ "scope": "markup.inserted", "settings": { "foreground": "#ffb3b8ee" } },
		{ "scope": "markup.deleted", "settings": { "foreground": "#c94d5a" } },
		{ "scope": "markup.changed", "settings": { "foreground": "#c94d5aee" } },
		{
			"scope": "constant.other.color",
			"settings": { "foreground": "#c94d5a" }
		},
		{ "scope": "string.regexp", "settings": { "foreground": "#c94d5a" } },
		{
			"scope": "constant.character.escape",
			"settings": { "foreground": "#c94d5a" }
		},
		{
			"scope": "punctuation.section.embedded, variable.interpolation",
			"settings": { "foreground": "#c94d5aee" }
		},
		{ "scope": "invalid.illegal", "settings": { "foreground": "#ffc4c9" } },
		{ "scope": "invalid.broken", "settings": { "foreground": "#0c0707" } },
		{ "scope": "invalid.deprecated", "settings": { "foreground": "#ffc4c9" } },
		{
			"scope": "invalid.unimplemented",
			"settings": { "foreground": "#ffc4c9" }
