	"tokenColors": [
		{
			"scope": "variable.parameter.function",
			"settings": { "foreground": "#f96688" }
		},
		{
			"scope": "comment, punctuation.definition.comment",
			"settings": { "foreground": "#6d4a5caa" }
		},
		{
			"scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.parameters, punctuation.definition.array",
			"settings": { "foreground": "#ffb3cc98" }
		},
		{ "scope": "none", "settings": { "foreground": "#ff85a3" } },
		{ "scope": "keyword.operator", "settings": { "foreground": "#ffa3ba" } },
		{ "scope": "keyword", "settings": { "foreground": "#d96b85ee", "fontStyle": "italic" } },
		{ "scope": "variable", "settings": { "foreground": "#ff7a99" } },
		{
			"scope": "entity.name.function, meta.require, support.function.any-method",
			"settings": { "foreground": "#ff8599", "fontStyle": "italic" }
		},
		{
			"scope": "support.class, entity.name.class, entity.name.type.class",
			"settings": { "foreground": "#ff7099" }
		},
		{ "scope": "meta.class", "settings": { "foreground": "#ffc4d9" } },
		{
			"scope": "keyword.other.special-method",
			"settings": { "foreground": "#7f3d52" }
		},
		{ "scope": "storage", "settings": { "foreground": "#d96b85ee" } },
		{ "scope": "support.function", "settings": { "foreground": "#d96b85" } },
