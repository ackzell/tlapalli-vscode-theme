		{ "scope": "constant", "settings": { "foreground": "#ff99ad" } },
		{ "scope": "entity.name.tag", "settings": { "foreground": "#804a5d" } },
		{
			"scope": "entity.other.attribute-name",
			"settings": { "foreground": "#ff8599" }
		},
		{
			"scope": "entity.other.attribute-name.id, punctuation.definition.entity",
			"settings": { "foreground": "#7f3d52" }
		},
		{ "scope": "meta.selector", "settings": { "foreground": "#d96b85ee" } },
		{ "scope": "none", "settings": { "foreground": "#ff8599" } },
		{
			"scope": "markup.heading punctuation.definition.heading, entity.name.section",
			"settings": { "foreground": "#7f3d52", "fontStyle": "" }
		},
		{ "scope": "keyword.other.unit", "settings": { "foreground": "#ff8599" } },
